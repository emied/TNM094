extends ../layout

include ../bike-dashboard/mixins

block content


  +sidenav
  +topnav('1')

  .container-fluid.px-4
    .row.justify-content-center
      .col-xs-12.col-sm-12.col-md-12.col-lg-12.d-flex.justify-content-center.align-items-center.p-2
        div(id="compressor-id", style='background-color: white').text-center.w-100.info-box.card-shadow
        div(id="avg-flow").text-center
      .container-fluid.col-xs-12.col-sm-12.col-md-12.col-lg-12.p-2.m-0
        +range-chart-card('line-chart-temp', 'Oil Temp', 'range-chart-oil-temp')
      
          
        
          
  script(src='javascripts/startup-data-request-compressor-id.js', type='module')
  script(src='javascripts/sidenav.js')
