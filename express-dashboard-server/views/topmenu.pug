.topnav
	span#menu-btn.glyphicon.glyphicon-menu-hamburger(onclick='toggleNav()')

	div(style="margin-left: auto;margin-right: auto;display: block; width: 40%")
		span#toggle-dots.glyphicon.glyphicon-map-marker(onclick='toggleStationDots()')
		a(href='dashboard?data=bike&view=1') View1
		a(href='dashboard?data=bike&view=2') View2
		a(href='dashboard?data=bike&view=3') View3

		.container.col-md-3.form-group#datetimepicker.input-group.date(style="padding: 6px 0px 0px 0px;float: left;margin-left: 10px")
			input(type='text', name='daterange', value='01/01/2018 - 01/15/2018', class="form-control", style="width: 180px")
			span.input-group-addon
				span.glyphicon.glyphicon-calendar

		span#menu-warning.glyphicon.glyphicon-bullhorn(onclick='toggleWarning()') 


